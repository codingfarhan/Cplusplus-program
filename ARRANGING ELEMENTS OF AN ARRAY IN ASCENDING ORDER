#include<iostream.h>
#include<conio.h>
int main()
{
int a[5], temp;
cout<<"input numbers"<<endl;
for(int i=0; i<5; i++)
{
cin>>a[i];
}

for(i=0; i<5; i++)
{
 for(int j=i+1; j<5; j++)
 {
  if (a[i]>a[j])
  {
  temp=a[i];
  a[i]=a[j];
  a[j]=temp;
  }
  else continue;
 }
}

cout<<"elements arranged in ascending order: "<<endl;
for(i=0; i<5; i++)
{
cout<<a[i]<<endl;
}
getch();
}
