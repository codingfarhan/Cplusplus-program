#include<iostream.h>
#include<conio.h>
#include<math.h>
float stdev(int a[5])
{
 float s=0, k=0, f, l;

 for(int i=0; i<5; i++)
 {
   s+=a[i];
 }

 for(i=0; i<5; i++)
 {
   k+=a[i]*a[i];
 }

 l=(k/5)-(s/5)*(s/5);
 f=sqrt(l);

 return f;
}

int main()
{

float stdev(int a[5]);
int w[5];
cout<<"enter the individual series"<<endl;

for(int i=0; i<5; i++)
{
  cin>>w[i];
}

cout<<"standard deviation of the entered individual series is: ";
float g=stdev(w);
cout<<g;
getch();
}
