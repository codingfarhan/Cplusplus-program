#include<iostream.h>
#include<conio.h>

void swap(int&a,int&b)
{
int temp;
temp=a;
a=b;
b=temp;
cout<<"\n"<<"b= "<<b<<"\n"<<"a= "<<a<<endl;
}
void main()
{
void swap(int&a,int&b);
int a, b;
cout<<"enter two numbers you want to swap"<<endl;
cin>>a>>b;
cout<<"a= "<<a<<"\n"<<"b= "<<b<<endl;
swap(a,b);
getch();
}
